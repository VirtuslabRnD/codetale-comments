        "parentId" : "30cd0cb8-185a-456d-8c16-4c968d6c676b",
        "authorId" : "6e2b5eda-1533-4798-a56c-432faaf38480",
        "body" : "DeltaFIFO is not required to return a valid object on delete.",
        "createdAt" : "2017-02-26T00:29:30Z",
        "updatedAt" : "2017-02-27T15:16:45Z",
        "lastEditedBy" : "6e2b5eda-1533-4798-a56c-432faaf38480",
