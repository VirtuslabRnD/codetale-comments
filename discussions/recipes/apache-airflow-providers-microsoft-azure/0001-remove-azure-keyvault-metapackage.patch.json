        "parentId" : "505206dd-bc5b-4f4f-ba76-cd111b8928a7",
        "authorId" : "0b815327-ca6e-45ac-956a-93a3957ac2f5",
        "body" : "Should we add this as a real package then?",
        "createdAt" : "2021-02-25T14:30:17Z",
        "updatedAt" : "2021-02-25T14:30:18Z",
        "lastEditedBy" : "0b815327-ca6e-45ac-956a-93a3957ac2f5",
