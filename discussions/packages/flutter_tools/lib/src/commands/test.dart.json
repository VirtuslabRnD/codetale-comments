    "diffHunk" : "@@ -1,1 +83,87 @@      ..addOption('concurrency',\n        abbr: 'j',\n        defaultsTo: math.max<int>(1, platform.numberOfProcessors - 2).toString(),\n        help: 'The number of concurrent test processes to run.',\n        valueHelp: 'jobs');"
  },
