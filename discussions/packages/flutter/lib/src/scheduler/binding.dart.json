        "parentId" : "62899bd3-48fe-4c8e-a720-648b66695b81",
        "authorId" : "0fc1222a-8e68-4923-bf2e-18203f0657f2",
        "body" : "OK, makes sense.",
        "createdAt" : "2019-06-14T18:16:07Z",
        "updatedAt" : "2019-06-14T18:16:08Z",
        "lastEditedBy" : "0fc1222a-8e68-4923-bf2e-18203f0657f2",
        "tags" : [
        ]
      }
