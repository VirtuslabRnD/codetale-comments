      {
        "id" : "858d305d-88a3-49f0-80ff-018de50d520b",
        "parentId" : "07353b41-c374-4638-bf7f-e26c87959eac",
        "authorId" : "189704fc-9fbb-4586-ae48-410635439866",
        "body" : "I was talking about this comment here. What kind of flexibility is it you wish to add?",
        "createdAt" : "2018-08-13T10:39:38Z",
        "updatedAt" : "2018-08-13T10:39:38Z",
        "lastEditedBy" : "189704fc-9fbb-4586-ae48-410635439866",
