    "diffHunk" : "@@ -1,1 +288,292 @@        tm.assert_series_equal(result, expected)\n\n        # convert to series while keeping the timezone\n        result = idx.to_series(keep_tz=True, index=[0, 1])\n        tm.assert_series_equal(result, expected)"
  }
