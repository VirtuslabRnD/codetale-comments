        "parentId" : null,
        "authorId" : "7086d5c0-382b-4c41-b70d-144a07643f71",
        "body" : "what is _cache for?",
        "createdAt" : "2018-06-19T00:30:07Z",
        "updatedAt" : "2018-06-21T10:23:50Z",
        "lastEditedBy" : "7086d5c0-382b-4c41-b70d-144a07643f71",
