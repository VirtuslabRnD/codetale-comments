    "diffHunk" : "@@ -1,1 +105,109 @@        *out_objs = PySequence_Fast_ITEMS(seq);\n        ret = PySequence_Fast_GET_SIZE(seq);\n        Py_SETREF(*out_kwd_obj, seq);\n        return ret;\n    }"
  },
